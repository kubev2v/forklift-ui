{
  "name": "forklift-ui",
  "version": "0.0.1",
  "description": "OpenShift VM Migration UI",
  "main": "deploy/server.js",
  "repository": "https://github.com/konveyor/forklift-ui.git",
  "license": "Apache",
  "private": true,
  "scripts": {
    "dr:surge": "node dr-surge.js",
    "build": "npm run clean && NODE_ENV=production $(npm bin)/webpack --config ./config/webpack.prod.js",
    "build:remote": "DATA_SOURCE=remote npm run build",
    "build:mock": "DATA_SOURCE=mock npm run build && npm run dr:surge",
    "setup:dev:remote": "node --trace-deprecation ./scripts/remote-dev.js",
    "start": "node --trace-deprecation deploy/server.js",
    "start:remote": "DATA_SOURCE=remote npm run start",
    "start:mock": "DATA_SOURCE=mock npm run start",
    "start:dev": "./scripts/start-dev.sh",
    "start:dev:remote": "npm run setup:dev:remote && DATA_SOURCE=remote npm run start:dev",
    "start:dev:mock": "DATA_SOURCE=mock npm run start:dev",
    "test": "npm run test:unit -- --coverage",
    "test:unit": "DATA_SOURCE=mock $(npm bin)/jest --rootDir=. --config=config/jest.config.js",
    "test:watch": "npm run test:unit -- --watch",
    "lint": "$(npm bin)/eslint --ext .tsx,.ts,.js ./src/ ./qe-tests/",
    "lint:fix": "npm run lint -- --fix",
    "format": "$(npm bin)/prettier --check --write ./src/**/*.{tsx,ts} ./qe-tests/**/*.{ts,js}",
    "tsc-src": "$(npm bin)/tsc --project ./tsconfig.json --noEmit",
    "tsc-qe": "$(npm bin)/tsc --project ./qe-tests/tsconfig.json --noEmit",
    "tsc": "npm run tsc-src && npm run tsc-qe",
    "type-check": "npm run tsc",
    "ci": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "$(npm bin)/webpack --config ./config/webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && $(npm bin)/webpack-bundle-analyzer ./stats.json",
    "clean": "$(npm bin)/rimraf dist"
  },
  "devDependencies": {
    "@axe-core/react": "^4.0.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/dotenv-webpack": "^3.0.0",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.16",
    "@types/q": "^1.5.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/simple-oauth2": "^4.1.0",
    "@types/victory": "^33.1.4",
    "@types/yup": "^0.29.7",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.5.3",
    "cypress": "^6.2.1",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.5.1",
    "imagemin": "^7.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.4",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-query-devtools": "^2.6.3",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "simple-oauth2": "^4.1.0",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@konveyor/lib-ui": "^1.11.0",
    "@patternfly/react-core": "^4.79.2",
    "@patternfly/react-icons": "^4.7.18",
    "@patternfly/react-styles": "^4.7.16",
    "@patternfly/react-table": "^4.19.24",
    "@patternfly/react-tokens": "^4.9.18",
    "axios": "^0.21.1",
    "dayjs": "^1.9.3",
    "dayjs-ext": "^2.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "fast-deep-equal": "^3.1.3",
    "http-proxy-middleware": "^1.0.6",
    "node-fetch": "^2.6.1",
    "q": "^1.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query": "^2.23.0",
    "react-router-last-location": "^2.0.1",
    "yup": "^0.29.3"
  }
}
