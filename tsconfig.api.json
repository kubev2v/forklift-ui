    "outDir": "dist",
    "rootDir": ".",
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "ES6",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "ts-node": {
    "files": true,
  },
  "files": [
    "./deploy/server.ts",
  ],
  "include": [
    "./deploy/server.ts",
  ],
  "exclude": ["node_modules", "qe-tests", "dist"]
}
